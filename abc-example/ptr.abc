fn printU64(:u64);
fn printNl();

fn main(): u8
{
    local a: u64 = 1,
	  b: u64 = 2,
	  c: u64 = 3;

    printU64(a); printNl();
    printU64(b); printNl();
    printU64(c); printNl();

    local ptr: -> u64 = &b;

    *(ptr + -1) = 42;

    printU64(a); printNl();
    printU64(b); printNl();
    printU64(c); printNl();

}
