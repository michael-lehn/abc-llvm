#include "stdio.abh"

fn factorial(n: u64): u64
{
    local r: u64 = 1;
    for (local i: u64 = 1; i <= n; i = i + 1) {
	r = r * i;
    }
    return r;
}

fn main(): u64
{
    local n: u64;
    
    printf("n = ");
    if (scanf("%llu", &n)) {
	printf("%llu! = %llu\n", n, factorial(n));
	return 0;
    }

    return 1;
}
