fn getU64(): u64;
fn getErr(): u64;

fn printU64(val: u64);
fn printNl();

fn factorial(n: u64): u64
{
    local r: u64 = 1;
    for (local i: u64 = 1; i <= n; i = i + 1) {
	r = r * i;
    }
    return r;
}

fn main(): u64
{
    local n: u64 = getU64();

    if (getErr() == 0) {
	printU64(factorial(n));
	printNl();
	return 0;
    }

    return 1;
}
