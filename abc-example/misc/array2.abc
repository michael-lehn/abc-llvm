#include "stdio.abh"

fn initArray(p: -> u64, dim: u64)
{
    for (local i: u64 = 0; i < dim; ++i) {
	p[i] = i;
    }
}

fn printArray(p: -> u64, dim: u64)
{
    for (local i: u64 = 0; i < dim; ++i) {
	printf("%llu ", p[i]);
    }
    printf("\n");
}

fn main(): u8
{
    local a: array[3] of u64;

    initArray(a, 3);
    printArray(a, 3);

    return 0;
}
