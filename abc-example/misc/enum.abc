#include <stdio.hdr>

enum Foo : i16
{
    A = 42,
    B,
    C = 13,
};

global p: u8 = B + C;

fn main()
{
    printf("A = %d\n", A);
    printf("B = %d\n", B);
    printf("C = %d\n", C);
    printf("p = %d\n", p);

    local err: -> i32;
    err = cast(B: -> i32);

    printf("err = %p\n", err);
}
